FROM python:3.9
WORKDIR /app
COPY . .
RUN pip install --upgrade pip
RUN pip install -r bagel/requirements.txt
RUN pip install ./bagel
RUN pip install -r bagel/sources/okta/requirements.txt
CMD ["bagel/sources/okta/sleep.py"]
ENTRYPOINT ["python"]
